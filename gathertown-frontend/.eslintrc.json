{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-undef": "warn" // This changes the rule for undefined variables to a warning instead of an error.
    },
    "globals": {
      "google": "readonly"
    }
  }
  